{
  "hooks": {
    "PreWriteFile": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/file_lock_manager.py"
          },
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/lock_wait_manager.py"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/pre_write_check.py"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/post_write_quality.py"
          },
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/post_write_notify.py"
          },
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/auto_retry_command.py"
          }
        ]
      },
      {
        "matcher": "Task",
        "hooks": [
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/task_logger.py"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/superclaude_persona_router.py"
          },
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/prompt_router.py"
          },
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/dna_resource_scanner.py"
          },
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/multi_implement_scanner.py"
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/superclaude_quality_gates.py"
          },
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/subagent_complete.py"
          },
          {
            "type": "command",
            "command": "cat | python3 $CLAUDE_PROJECT_DIR/.claude/hooks/multi_team_subagent_complete.py"
          }
        ]
      }
    ]
  }
}
